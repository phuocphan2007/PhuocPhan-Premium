	function auto_Ability()
		while _G.auto_Ability == true do
			local args = {
				[1] = "ActivateAbility"
			}
			game:GetService("ReplicatedStorage").Remotes.CommE:FireServer(unpack(args))
			wait(27)
		end
	end

	function Meele_Stat()
		while _G.Meele_Stat == true do
			local args = {
				[1] = "AddPoint",
				[2] = "Melee",
				[3] = 1
			}
			game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
			wait(0)
		end
	end

