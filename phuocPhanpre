--------------------------------------------------BUY FRUIT--------------------------------------------
function auto_buy_Fruit()
	while _G.auto_buy_Fruit == true do
		local args = {
			[1] = "PurchaseRawFruit",
			[2] = "Kilo-Kilo",
			[3] = false
		}
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))(_G.Select_Fruit)
		
		local args = {
			[1] = "PurchaseRawFruit",
			[2] = "Spin-Spin",
			[3] = false
		}
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))(_G.Select_Fruit)
		
		local args = {
			[1] = "PurchaseRawFruit",
			[2] = "Chop-Chop",
			[3] = false
		}
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))(_G.Select_Fruit)
		
		local args = {
			[1] = "PurchaseRawFruit",
			[2] = "Spring-Spring",
			[3] = false
		}
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))(_G.Select_Fruit)
		
		local args = {
			[1] = "PurchaseRawFruit",
			[2] = "Spike-Spike",
			[3] = false
		}
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))(_G.Select_Fruit)
		
		local args = {
			[1] = "PurchaseRawFruit",
			[2] = "String-String",
			[3] = false
		}
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))(_G.Select_Fruit)
		
		local args = {
			[1] = "PurchaseRawFruit",
			[2] = "Portal-Portal",
			[3] = false
		}
		game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))(_G.Select_Fruit)
		wait()
	end
end


